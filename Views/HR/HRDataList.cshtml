@{
}

@model List<leads_hr_ltd.Models.Employee>

@{
    ViewData["Title"] = "Hr Data  ";
}

<h2>HR Data List</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Division</th>
            <th>Building</th>
            <th>Title</th>
            <th>Room</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.FirstName</td>
                <td>@employee.LastName</td>
                <td>@employee.Division</td>
                <td>@employee.Building</td>
                <td>@employee.Title</td>
                <td>@employee.Room</td>
            </tr>
        }
    </tbody>
</table>

